<h1>NEW PAGE</h1>

<%= form_tag "/contacts", method: :post do %>
<div class="form-group">
  Firs Name: <input type="text" class="form-control" name="form_first_name">
</div>
<div class="form-group">
  Last Name: <input type="text" class="form-control" name="form_last_name">
</div>
<div class="form-group">
  Email: <input type="text" class="form-control" name="form_email">
</div>
<div class="form-group">
  Phone: <input type="text" class="form-control" name="form_phone">
</div>
<input type="submit" class="btn btn-default" value="create contact">


<% end %>